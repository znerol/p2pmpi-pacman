<?xml version="1.0" encoding="UTF-8"?>
<project name="deism-build-javadoc" default="javadoc" basedir=".">
    <target name="javadoc">
        <mkdir dir="doc/javadoc/"/>
        <javadoc sourcepath="src" overview="src/overview.html" destdir="doc/javadoc/">
        </javadoc>
    </target>
    <target name="clean">
        <delete dir="doc/javadoc"/>
    </target>
</project> 